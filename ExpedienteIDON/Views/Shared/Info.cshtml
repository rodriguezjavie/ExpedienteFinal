@{
    ViewBag.Title = "Info";
}
<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <div class="page-title-breadcrumb">
                <div class=" pull-left">
                    <div class="page-title">Error</div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="card card-box">
                    <div class="card-body ">
                        <h2>@ViewBag.Title.</h2>
                        <h3>@ViewBag.Message</h3>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>