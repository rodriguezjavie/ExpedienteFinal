<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- google font -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet" type="text/css" />
    <!-- icons -->
    <link href="@Url.Content("/Scripts/theme/fonts/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <!-- <link rel="stylesheet" href="../assets/plugins/iconic/css/material-design-iconic-font.min.css"> -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/material-design-iconic-font/2.2.0/css/material-design-iconic-font.min.css">
    <!-- bootstrap -->
    <link href="@Url.Content("/Scripts/theme/plugins/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <!-- style -->
    <link href="@Url.Content("/Scripts/theme/css/pages/extra_pages.css")" rel="stylesheet" type="text/css" />
</head>
<body>
    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <!-- start js include path -->
    <script src="@Url.Content("/Scripts/theme/plugins/jquery/jquery.min.js")"></script>
    <!-- bootstrap -->
    <script src="@Url.Content("/Scripts/theme/plugins/bootstrap/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("/Scripts/theme/js/pages/extra_pages/extra_pages.js")"></script>
    <!-- end js include path -->
</body>
</html>

